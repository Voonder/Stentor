<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	android:id="@+id/item_licence_layout"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:minHeight="@dimen/item_min_height"
	android:orientation="vertical"
	android:paddingBottom="20dp"
	android:paddingEnd="@dimen/margin_app"
	android:paddingStart="@dimen/margin_app"
	android:paddingTop="@dimen/margin_app">

	<TextView
		android:id="@+id/item_license_name"
		style="@android:style/TextAppearance.Material.Body1"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="@string/license_master_mit"
		android:textSize="16sp"/>


</LinearLayout>